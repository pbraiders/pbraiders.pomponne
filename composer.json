{
    "name": "pbraiders/pomponne",
    "description": "A paintball reservation management system",
    "keywords": [
        "paintball",
        "reservation",
        "management"
    ],
    "homepage": "https://github.com/pbraiders/pomponne",
    "readme": "README.md",
    "time": "2019-10-14",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Olivier Jullien",
            "homepage": "https://github.com/ojullien",
            "email": "contact@pbraiders.fr"
        }
    ],
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/pbraiders/stdlib"
        },
        {
            "type": "github",
            "url": "https://github.com/pbraiders/container"
        },
        {
            "type": "github",
            "url": "https://github.com/pbraiders/config"
        }
    ],
    "require": {
        "php": "^7.3",
        "filp/whoops": "^2.5",
        "league/plates": "^4.0.0-alpha",
        "league/uri-parser": "^1.4",
        "monolog/monolog": "^2.0",
        "pbraiders/config": "^1.0",
        "pbraiders/container": "^1.0",
        "pbraiders/stdlib": "^1.0",
        "php-di/php-di": "^6.0",
        "php-di/slim-bridge": "^3.0",
        "slim/psr7": "^0.6",
        "slim/slim": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Pbraiders\\Pomponne\\": "pbr/"
        }
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^8"
    },
    "autoload-dev": {
        "psr-4": {
            "PbraidersTest\\Pomponne\\": "tests/pbr/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "vendor-dir": "lib"
    },
    "scripts": {
        "test": "lib/bin/phpunit --configuration ./phpunit.xml.dist --coverage-clover build/logs/clover.xml"
    }
}
